package automation;
import org.openqa.selenium.By;
import org.openqa.selenium.WebDriver;
import org.openqa.selenium.WebElement;
import org.openqa.selenium.chrome.ChromeDriver;

public class Sewebelements {

	public static void main(String[] args) {

		WebDriver browser=new ChromeDriver();
		browser.get("https://parabank.parasoft.com/parabank/index.htm");
		WebElement user_textfield = browser.findElement(By.xpath("//input[@name='username']"));//WebElement user_textfield =browser.findElement(By.className("input_username"));
		user_textfield.sendKeys("SHYA@123");
		WebElement pass_textfield = browser.findElement(By.xpath("[//input[@name='password']"));//WebElement pass_textfield =browser.findElement(By.className("input_password"));
		pass_textfield.sendKeys("Shya#$7876");
		WebElement login =browser.findElement(By.xpath("//input[@value='Login']"));//WebElement login =browser.findElement(By.className("btn_action"));
    System.out.println("Login button text: " + login.getAttribute("value"));
	   login.click();
     browser.close();   
	}

}
